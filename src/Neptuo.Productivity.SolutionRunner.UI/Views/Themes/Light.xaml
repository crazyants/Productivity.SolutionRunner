<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundBrush">#F5F5F5</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundBrush">#1E1E1E</SolidColorBrush>
    <Color x:Key="ActiveColor">#569DE5</Color>
    <SolidColorBrush x:Key="ActiveBrush">#569DE5</SolidColorBrush>
    <Color x:Key="InactiveColor">#FFFFFF</Color>
    <SolidColorBrush x:Key="InactiveBrush">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="HoverBrush">#663683D3</SolidColorBrush>
    <SolidColorBrush x:Key="TextBoxInactiveBrush">#ABADB3</SolidColorBrush>
    <SolidColorBrush x:Key="TextBoxBackgroundBrush">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="GrayBrush">#777777</SolidColorBrush>
    <SolidColorBrush x:Key="LinkForegroundBrush">#4A9AD4</SolidColorBrush>
</ResourceDictionary>